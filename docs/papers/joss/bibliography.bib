	shortjournal = {Nuclear Engineering and Design},
	author = {Che, Yifeng and Pastore, Giovanni and Hales, Jason and Shirvan, Koroush},
	date = {2018-10-01},
	file = {Che et al. - 2018 - Modeling of Cr2O3-doped UO2 as a near-term acciden.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/BWG9R5JZ/Che et al. - 2018 - Modeling of Cr2O3-doped UO2 as a near-term acciden.pdf:application/pdf;ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/CESW9WRX/S002954931830102X.html:text/html}
}

@report{barres_effet_2018,
	location = {{CEA} Saclay},
	title = {Effet des fortes teneurs en hydrogène sur les propriétés mécaniques des gaines en M5® après incursion à haute température},
	abstract = {Cette note présente les résultats des essais de caractérisation mécanique effectués dans le cadre de la
thèse d’Isabelle Turque (mais non intégrés à son manuscrit de thèse), à différentes températures entre 20
et 700°C, au refroidissement après incursion dans le domaine de phase β du zirconium, sur des échantillons
issus de tubes en M5®, chargés ou non en hydrogène de manière homogène entre 1700 et 3200 ppm
mass. Les examens au microscope électronique à balayage et les analyses à la microsonde de Castaing
effectués depuis sur ces échantillons, ainsi que sur leurs équivalents en Zircaloy-4, sont également
présentés. Les résultats sur les échantillons base M5® sont dans l’ensemble cohérents avec ceux obtenus
dans le cas du Zircaloy-4.
Le module d'Young diminue lorsque la température augmente et n’est pas modifié de manière significative
par l’hydrogène. La ductilité du matériau augmente avec la température. Le matériau est fragilisé par
l’hydrogène en-dessous de 500°C. Il est macroscopiquement fragile aux températures inférieures ou égales
à 135°C pour des teneurs en hydrogène de 1720-1980 ppm mass. et aux températures inférieures ou
égales à 350°C pour des teneurs en hydrogène de 3090-3180 ppm mass. (sachant néanmoins que la
couche ex-β des échantillons en M5® contenant {\textasciitilde}3200 ppm mass. d’hydrogène testés à 135 et 350°C
contenait environ 0,6 et 0,8 \% mass. d’oxygène en moyenne). L’hydrogène n’a plus d’effet fragilisant au-
delà de 500°C. Quand le matériau est macroscopiquement ductile, la contrainte d’écoulement du matériau
pré-hydruré à environ 2000-3000 ppm mass. est plus élevée que celle du matériau non pré-hydruré en-
dessous de 500°C et plus faible à 700°C (à cette température se situant au-dessus du palier « eutectoïde »
du diagramme de phases Zr-H, les matériaux plus ou moins fortement hydrurés sont partiellement
transformés en phase β). Dans ce cas, la localisation de la déformation (striction) intervient pour un niveau
de déformation plus faible dans le cas du matériau fortement hydruré.
Le mode de rupture de la structure ex-β fortement pré-hydrurée est fortement lié à sa microstructure,
constituée de zones enrichies en hydrogène et appauvries en oxygène et de zones moins riches en
hydrogène mais plus riches en oxygène, du fait de la partition des éléments chimiques lors de la
transformation de phases β ⟶ α du zirconium au refroidissement},
	number = {{NT} 2018-3762/A},
	institution = {{CEA} {DEN}/{DANS}/{DMN}/{SRMA}/{LC}2M},
	type = {Note technique},
	author = {Barrès, Quentin and Turque, Isabelle and Le Saux, Matthieu},
	date = {2018-07-04},
	file = {Barrès et al. - 2018 - Effet des fortes teneurs en hydrogène sur les prop.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/8PW8C8JB/Barrès et al. - 2018 - Effet des fortes teneurs en hydrogène sur les prop.pdf:application/pdf}
}

@thesis{robert-berat_influence_2001,
	title = {Influence d'une couche de Zircone sur le comportement mécanique des tubes en Zircaloy-4},
	abstract = {Au cœur des Réacteurs à Eau sous Pression, la gaine en Zircaloy-4 est soumise à la corrosion : une couche d’oxyde externe et
une couche d’oxyde interne se forment. Celles-ci sont susceptibles d’influencer le comportement des tubes pendant leur séjour en
réacteur et pendant leur entreposage. Le but de cette étude est donc de contribuer à la compréhension du comportement mécanique des
tubes oxydés.
Des essais mécaniques sont effectués sur des tubes de gainage et tubes guides en traction et pression interne. Ils mettent en
évidence un effet différent de l’oxyde sous ces deux types de sollicitation. En traction, la couche de zircone entraîne un important
renfort. En pression interne, elle entraîne soit un faible renfort, soit un affaiblissement. La fissuration de la couche d’oxyde est
caractérisée et quantifiée par des observations au {MEB} en fin d’essais. Ces observations mettent en évidence la nécessité d’une
modélisation du comportement prenant en compte l’endommagement par fissuration de la couche.
Un modèle aux éléments finis est donc présenté pour un tube recouvert de deux couches de zircone (interne et externe) non
fissurées puis fissurées. Pour expliquer cette différence de comportement suivant le type de sollicitation appliquée, l’hypothèse d’un
effet de type « structure » est proposée. La présence des couches d’oxyde fissurées entraîne des contraintes circonférentielles dans le
Zircaloy qui favorisent un renfort en traction uni-axiale, malgré la fissuration de la couche d’oxyde. Alors qu’en pression interne, la
présence de l’oxyde entraîne des contraintes axiales dans le Zircaloy qui s’opposent à la diminution des déformations circonférentielles,
limitant ainsi l’effet de renfort qui est alors nettement plus faible qu’en traction. L’influence de l’oxyde sur les contraintes s’exerçant sur
le Zircaloy est donc à l’origine de cette différence de comportement en traction et pression interne.},
	institution = {Université Blaise Pascal},
	type = {phdthesis},
	author = {Robert-Berat, Laurent},
	date = {2001-06},
	file = {Robert-Berat - 2001 - Influence d'une couche de Zircone sur le comportem.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/9XTBQ6SE/Robert-Berat - 2001 - Influence d'une couche de Zircone sur le comportem.pdf:application/pdf}
}

