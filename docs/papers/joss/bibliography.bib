	author = {Diani, J. and Brieu, M. and Vacherand, J. -M. and Rezgui, A.},
	date = {2004-04-01},
	keywords = {Anisotropy, Hyperelasticity, Elastomer, Directional model, Biaxial tension, Strain energy density},
	file = {Diani et al. - 2004 - Directional model for isotropic and anisotropic hy.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/JZG8VINC/Diani et al. - 2004 - Directional model for isotropic and anisotropic hy.pdf:application/pdf;ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/499E2QSP/S0167663603000255.html:text/html}
