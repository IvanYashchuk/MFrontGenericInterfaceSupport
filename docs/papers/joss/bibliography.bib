	langid = {english},
	keywords = {Fracture, Finite elements, Damage, Non-local models, Phase field, Stability principle, Variational methods},
	file = {Marigo et al. - 2016 - An overview of the modelling of fracture by gradie.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/NQQJDQAQ/Marigo et al. - 2016 - An overview of the modelling of fracture by gradie.pdf:application/pdf}
}

