	eprint = {physics/0610206},
	keywords = {Physics - Computational Physics, High Energy Physics - Phenomenology, Mathematics - Numerical Analysis},
	file = {arXiv.org Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/3CT3CDHX/0610206.html:text/html;Kopp - 2008 - Efficient numerical diagonalization of hermitian 3.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/8J23497G/Kopp - 2008 - Efficient numerical diagonalization of hermitian 3.pdf:application/pdf}
