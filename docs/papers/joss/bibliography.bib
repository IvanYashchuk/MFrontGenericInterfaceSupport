	keywords = {Mild steel, {RK} Constitutive relation, Thermo-visco-plasticity, Consistent algorithm, Dynamic plasticity, Numerical simulations, Finite element code},
	file = {Rusinek et al. - 2007 - Constitutive relations in 3-D for a wide range of .pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/EAV64UCQ/Rusinek et al. - 2007 - Constitutive relations in 3-D for a wide range of .pdf:application/pdf;ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/FD326VF8/S0020768307000236.html:text/html}
