@report{derouiche_integration_2018,
	title = {Intégration simplifiée de modèles de comportement non-linéaire},
	abstract = {L’objectif principal de cette étude était d’évaluer la pertinence et la robustesse
de l’intégration d’un modèle de comportement élasto-viscoplastique présentant une
forte non-linéarité dans la filière de dimensionnement à la fatigue thermomécanique.
Actuellement, le groupe {PSA} utilise un {USER} Material ({UMAT}) codé en Fortran
pour modéliser le modèle élasto-viscoplastique des aluminiums. Or l’intégration d’un
nouveau modèle de comportement via un {UMAT} Fortran passe par une mise en
données complexe et génératrice d’erreurs. Des solutions d’intégration numérique
rapides ont vu le jour pour faciliter l’intégration des modèles les plus complexes
notamment {MFront}.
Comme première étape du travail, nous avons formulé et intégré le modèle de compor-
tement élasto-viscoplastique des aluminiums du groupe {PSA} dans le logiciel {MFront}.
Les cas tridimensionnel et contraintes planes ont été étudiés où nous avons testé les
performances de trois méthodes d’intégration différentes. Une fois la formulation et
l’intégration réalisées, nous avons validé la justesse de nos équations constitutives
avec des calculs sur un point d’intégration. Ensuite, des calculs sur des structures
industrielles (éprouvette thermique, culasse monocylindre et culasse trois cylindres)
sont effectués sur le code élément fini {ABAQUS} afin de nous assurer de la convergence
rapide et systématique du modèle, de vérifier les équations constitutives de ce dernier
et de comparer la méthodologie actuelle du groupe {PSA} avec celles proposées par
{MFront}. Les différents algorithmes testés sont efficaces et donnent des résultats
similaires à celui du {UMAT} Fortran et avec des temps de calcul comparables et des
coûts de développement plus réduits},
	institution = {{PSA}},
	type = {Rapport de stage},
	author = {Derouiche, Khouloud},
	date = {2018-09-12},
	file = {Derouiche - 2018 - Intégration simplifiée de modèles de comportement .pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/WQAFDVEQ/Derouiche - 2018 - Intégration simplifiée de modèles de comportement .pdf:application/pdf}
}

@article{miehe_phase_2010-1,
	title = {A phase field model for rate-independent crack propagation: Robust algorithmic implementation based on operator splits},
	volume = {199},
	issn = {0045-7825},
	url = {http://www.sciencedirect.com/science/article/pii/S0045782510001283},
	doi = {10.1016/j.cma.2010.04.011},
	shorttitle = {A phase field model for rate-independent crack propagation},
	abstract = {The computational modeling of failure mechanisms in solids due to fracture based on sharp crack discontinuities suffers in situations with complex crack topologies. This can be overcome by a diffusive crack modeling based on the introduction of a crack phase field. Following our recent work [C. Miehe, F. Welschinger, M. Hofacker, Thermodynamically-consistent phase field models of fracture: Variational principles and multi-field fe implementations, International Journal for Numerical Methods in Engineering {DOI}:10.1002/nme.2861] on phase-field-type fracture, we propose in this paper a new variational framework for rate-independent diffusive fracture that bases on the introduction of a local history field. It contains a maximum reference energy obtained in the deformation history, which may be considered as a measure for the maximum tensile strain obtained in history. It is shown that this local variable drives the evolution of the crack phase field. The introduction of the history field provides a very transparent representation of the balance equation that governs the diffusive crack topology. In particular, it allows for the construction of a new algorithmic treatment of diffusive fracture. Here, we propose an extremely robust operator split scheme that successively updates in a typical time step the history field, the crack phase field and finally the displacement field. A regularization based on a viscous crack resistance that even enhances the robustness of the algorithm may easily be added. The proposed algorithm is considered to be the canonically simple scheme for the treatment of diffusive fracture in elastic solids. We demonstrate the performance of the phase field formulation of fracture by means of representative numerical examples.},
	pages = {2765--2778},
	number = {45},
	journaltitle = {Computer Methods in Applied Mechanics and Engineering},
	shortjournal = {Computer Methods in Applied Mechanics and Engineering},
	author = {Miehe, Christian and Hofacker, Martina and Welschinger, Fabian},
	urldate = {2016-07-01},
	date = {2010-11-15},
	keywords = {Fracture, Coupled multi-field problem, Finite elements, Crack propagation, Phase fields, Gradient-type damage, Incremental variational principles}
}

