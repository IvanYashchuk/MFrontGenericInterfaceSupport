	journaltitle = {Underground Space},
	shortjournal = {Underground Space},
	author = {Liu, K. and Chen, S. L.},
	date = {2017-09-01},
	keywords = {Finite element method, Strain-hardening Drucker–Prager model, {UMAT} code, Tunnel excavation},
