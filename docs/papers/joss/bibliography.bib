	author = {Magnier, V. and Charkaluk, E. and de Saxcé, G.},
	date = {2014-08-01},
	keywords = {Plasticity, Return mapping, bipotential, Non-associated laws, Numerical implementation, Abaqus},
	file = {Magnier et al. - 2014 - Bipotential versus return mapping algorithms Impl.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/M5Z72CWV/Magnier et al. - 2014 - Bipotential versus return mapping algorithms Impl.pdf:application/pdf;ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/RNNRPETJ/S002076831400153X.html:text/html}
