	issn = {0749-6419},
	url = {http://www.sciencedirect.com/science/article/pii/S0749641908001502},
	doi = {10.1016/j.ijplas.2008.09.007},
	series = {Exploring New Horizons of Metal Forming Research},
	abstract = {Sheet metal forming processes generally involve non-proportional strain paths including springback, leading to the Bauschinger effect, transient hardening, and permanent softening behavior, that can be possibly modeled by kinematic hardening laws. In this work, a stress integration procedure based on the backward-Euler method was newly derived for a nonlinear combined isotropic/kinematic hardening model based on the two-yield’s surfaces approach. The backward-Euler method can be combined with general non-quadratic anisotropic yield functions and thus it can predict accurately the behavior of aluminum alloy sheets for sheet metal forming processes. In order to characterize the material coefficients, including the Bauschinger ratio for the kinematic hardening model, one element tension–compression simulations were newly tried based on a polycrystal plasticity approach, which compensates extensive tension and compression experiments. The developed model was applied for a springback prediction of the {NUMISHEET}’93 2D draw bend benchmark example.},
	pages = {1684--1710},
	number = {9},
	journaltitle = {International Journal of Plasticity},
	shortjournal = {International Journal of Plasticity},
	author = {Cardoso, Rui P. R. and Yoon, Jeong Whan},
	date = {2009-09-01},
	keywords = {Anisotropy, nonlinear kinematic hardening, Backward-Euler method, Polycrystal plasticity, Springback},
	file = {Cardoso et Yoon - 2009 - Stress integration method for a nonlinear kinemati.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/VPMW5V92/Cardoso et Yoon - 2009 - Stress integration method for a nonlinear kinemati.pdf:application/pdf;ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/QWSGHHZZ/S0749641908001502.html:text/html}
}

