@report{helfer_implementation_2017-1,
	location = {{CEA} Cadarache},
	title = {Implémentation au format {MFront} et vérification d'une loi viscoplastique pour la modélisation de gaines M5 en situation de transport et d'entreposage},
	abstract = {Dans le cadre du projet {PRECCI}, et dans le but de prédire le comportement des gaines M5 soumises à une pression interne lors des phases de transport et d’entreposage des crayons combustibles {REP}, une loi de comportement viscoplastique (pour le M5) a été établie en 2006 par le {DMN} (C. Cappelaere et R. Limon [3]) puis réidentifiée en 2009 (C. Cappelaere [2]) se basant alors sur une centaine d’essais couvrant des contraintes, des températures et des durées d’irradiation variées.
L’objectif de cette note est de décrire l’intégration au format {MFront} [5] de cette loi. Après avoir brièvement rappelé les équations constitutives de cette loi, nous présentons sa discrétisation temporelle.
Le choix a alors été fait de résoudre les équations du modèle discrétisé à l’aide d’un algorithme implicite et d’une θ-méthode qui peut être réduit à un système non linéaire de dimension 8.
Ce système d’équations non linéaires est résolu à l’aide d’une méthode de Newton avec Jacobienne analytique. L’algorithme de résolution a ensuite été optimisé (diminution de la dimension du système discrétisé, limitation des appels aux fonctions coûteuses en temps de calcul, ...) de manière à réduire les temps de calcul.
Des vérifications ont été faites pour garantir la validité de notre implémentation sous {MFront} [5]. Nous avons (entre autres) réalisé une comparaison à une implantation Mistral [1] de la loi (développée en 2006) donnant des résultats identiques, et ce pour un grand nombre de simulations avec une large variété de paramètres.
Une analyse de performances à l’aide de l’outil valgrind [4] a par ailleurs été réalisée, elle met en évidence que notre implémentation sous {MFront} [5] est particulièrement performante.
Cette implémentation et ces cas tests sont capitalisés dans le projet {MFrontMaterials}. Elle est utilisable dans Alcyone et Licos. Cette loi permet de décrire le comportement de la gaine dans les expériences {CROCODILE} ({FAB}\_CS et {FAB}\_BS).},
	number = {17-002},
	institution = {{DEN}/{DEC}/{SEC}/{LSC}},
	type = {Note technique},
	author = {Helfer, Thomas},
	date = {2017-07},
	file = {Helfer - Implémentation au format MFront et vérification d'.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/BNXQ6FXF/Helfer - Implémentation au format MFront et vérification d'.pdf:application/pdf}
}

@report{fayette_programme_2017,
	location = {{CEA} Cadarache},
	title = {Programme {CROCO} {TRANSPORT} M5 : résultats des examens métallographiques sur le crayon mox à gainage m5 fab-cs après essai de fluage à 450°C},
	abstract = {Cette note présente les résultats de trois examens métallographiques (2 Coupes Radiales {CR} bas et {CR}
haut et une Coupe Axiale {CA}) réalisés sur le crayon Fabrice {FAB}-{CS} (issu du crayon {FXP}2CC/E02 de type
combustible {MOX} avec une gaine M5 et irradié 4 cycles) après l’expérience de chauffe à 450°C pendant 4 0
jours cumulés dans le four {CROCODILE} sous atmosphère inerte d’argon. Le crayon {FAB}-{CS} présentait,
avant l’essai de chauffe, un jeu combustible/gaine ouvert en bas de crayon et fermé en haut. Suite à l’essai
de fluage, l’augmentation diamétrale de la gaine est mesurée à environ 0,74\% sur la {CR} bas, 0,95\% sur la
{CA} et 1,38\% sur la {CR} haut qui, compte tenu d’un gradient de température dans le four, ont été
respectivement soumis à une température de 450°C, 4 51°C et 455°C.
Les examens métallographiques montrent que
- la déformation diamétrale de la gaine se répartit de manière homogène sur sa circonférence et qu’il
n’y a aucun endommagement visible dans la partie de la gaine non oxydée.
- Concernant la fissuration de la pastille combustible, pour la {CR} bas, l’essai de chauffe a provoqué
un élargissement des fissures principales mais il n’a pas conduit à une fracturation additionnelle
importante en raison de l’absence d’un accrochage combustible/gaine avant l’essai. Pour la {CR} haut,
l’essai de chauffe a provoqué une fragmentation notable du combustible notamment en périphérie de
pastille en raison d’un accrochage au moins partiel du combustible à la gaine et d’une déformation
diamétrale plus importante de la gaine. Pour la {CA}, l’ouverture des fissures principales concerne la
totalité de la hauteur des pastilles et elle est équivalente entre les zones d’interpastilles et les
pastilles. Des ramifications et des fissures circonférentielles additionnelles sont visibles en périphérie
des pastilles mais ce phénomène est moins marqué que pour la {CR} haut compte tenu d’un ancrage
combustible/gaine et d’un taux de déformation plus faibles.
- Concernant l’ancrage du combustible dans la gaine, les trois métallographies font état d’un jeu
ouvert entre le combustible et la gaine. Pour la {CR} bas, le volume libre généré par la déformation
diamétrale de 0,74\% est majoritairement présent dans le jeu combustible/gaine. Pour la {CA}, le volume
libre généré par la déformation diamétrale intermédiaire de 0,95\% se répartit entre le jeu
combustible/gaine et l’élargissement de certaines fissures du réseau principal de la pastille. Pour la
{CR} haut, qui présente également un début d’ancrage du combustible dans la gaine avant la chauffe, le
jeu est très anisotrope, et le volume libre généré par la déformation diamétrale de 1,38\% se répartit
entre la ré-ouverture d’un jeu combustible/gaine et l’apparition de nouvelles fissures, notamment en
périphérie de la pastille combustible. Le fluage de la gaine observé n’a visiblement pas été impacté
par l’accrochage du combustible à la gaine.
- la couche de zircone externe montre des signes de dégradation pour les trois examens en raison de
l’état de contraintes généré par le fluage de la gaine. Des surépaisseurs de zircone, imputables à une
atmosphère au sein du four insuffisamment inerte lors du traitement thermique, sont visibles côté
métal.
- Sous l’effet des contraintes dues à la pression interne et du recuit, les hydrures sont orientés de
manière aléatoire et leur répartition est homogène dans l’épaisseur de la gaine.
- Les mesures de micro-dureté Vickers de la gaine montrent un adoucissement du matériau consécutif
à l’essai de chauffe. Cet adoucissement est lié à une restauration au moins partielle des défauts
d’irradiation, qui induisent un durcissement du matériau de gainage.},
	number = {{NT} 17-008},
	institution = {{DEN}/{CAD}/{DEC}/{SA}3C/{LEMCI}},
	type = {Note technique},
	author = {Fayette, Laurent and Reboul, Stéphane and Binet, Paul},
	date = {2017-09},
	file = {Fayette et al. - 2017 - Programme CROCO TRANSPORT M5  résultats des exame.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/XDTWQGFS/Fayette et al. - 2017 - Programme CROCO TRANSPORT M5  résultats des exame.pdf:application/pdf}
}

@report{aboikoni_conception_2016,
	title = {Conception d'un dispositif d'essai mécanique en température sur combustible (U,Pu)O2},
	abstract = {L’objet de cette note technique est de présenter l’étude de conception d’un dispositif d’essai thermomécanique sous atmosphère contrôlée devant être installé dans une boite à gants ({BàG}03) existante au sein du laboratoire L26 de l’{INB} Atalante. L’objectif est de pouvoir à terme réaliser des études mécaniques sur des combustibles (U,Pu)O 2-x dans le domaine viscoplastique afin d’alimenter les codes combustibles en lois de comportement. Dans un premier temps, une étude bibliographique a permis de cerner les domaines
de contrainte et de température souhaités pour dispositif (120 ≤ σ max ≤ 310 {MPa} et 1000 ≤ T ≤ 1800°C). Dans un second temps, une analyse fonctionnelle servant de base à un futur cahier des charges du dispositif d’essai thermomécanique a pu être établie. La conception a permis de déterminer le meilleur compromis dimensionnel afin de livrer un premier modèle de dispositif d’essai en accord avec les contraintes géométriques de la {BàG}03. L’ensemble expérimental basé sur une géométrie en bras de levier combinerait un vérin électromécanique et un four étanche pouvant fonctionner sous atmosphère contrôlée. Des premiers calculs mécaniques par éléments finis ont permis de localiser les zones à fortes contraintes du dispositif et d’obtenir un premier dimensionnement (masse, encombrement) de ce dernier.
Les perspectives de ce projet sont une continuité du dimensionnement mécanique des éléments du dispositif
finalisant la conception {CAO} de la machine d’essai. Il sera par la suite rédigé un cahier des charges fournisseurs, se basant sur celui présenté dans ce rapport, afin de permettre l’achat et l’installation des différents éléments de la machine d’essai pour réaliser les premiers essais de validation du dispositif.},
	number = {{DMRC}/{SFMA} {NT}/2017-08},
	institution = {{CEA} {DEN}/{MAR}/{DMRC}/{SFMA}/{LCC}},
	type = {Note technique},
	author = {Aboikoni, Albert and Martin, Phlippe},
	date = {2016-10-27},
	file = {Aboikoni et Martin - 2016 - Conception d'un dispositif d'essai mécanique en te.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/8RM4ZQEB/Aboikoni et Martin - 2016 - Conception d'un dispositif d'essai mécanique en te.pdf:application/pdf}
}

@article{oterkus_peridynamic_2017,
