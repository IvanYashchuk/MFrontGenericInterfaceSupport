	file = {Vidal - 2015 - Synthèse des lois de viscoplasticité développées p.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/VPMHI5CW/Vidal - 2015 - Synthèse des lois de viscoplasticité développées p.pdf:application/pdf}
}

