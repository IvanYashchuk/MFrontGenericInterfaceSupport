@report{dumerval_bilan_2016,
	location = {{CEA} Saclay},
	title = {Bilan des essais {EDGAR} multi-crayons réalisés en 2015 et en 2016},
	abstract = {Cette note présente le bilan des essais réalisés dans l’installation {EDGAR} multi-crayons dans le cadre du
projet Gaines Lot 3 en 2015 et 2016.
La première partie de cette note présente les essais de fluage réalisés sur gaines en M5® vierge à 800°C
et 50 bars de pression relative. Ces tests ont permis de mesurer ou de déduire l’écart thermique azimutal
maximal pour 4 configurations différentes :
- 4 gardes : 63°C
- 2 gardes à l’opposé : 97 à 112 °C
- 2 gardes côte à côte : 121 à 166°C
- 1 garde : 109 à 140°C
Les mesures post-mortem ont permis de montrer que les déformations circonférentielles moyennes à
rupture (déformation maximale At, déformation à +/- 20 mm de l’éclatement et déformation en partie
courante) diminuent lorsque le gradient azimutal de température augmente.
La deuxième partie est consacrée aux essais de fluage réalisés à 700°C sous 60 bars sur gaines en M5®
vierge avec 4 gardes, avec ou sans noyau interne en alumine. Ces essais ont permis de montrer que, dans
ces conditions :
- L’effet de la présence de gardes (configuration multi-crayons), avec ou sans noyau d’alumine, est du
premier ordre sur les déformations post-mortem (At et Ar) mesurées à l’issue d’un essai, qui sont plus
faibles que celles mesurées après un essai mono-crayon.
- En configuration multi-crayons, l’effet du noyau semble être du second ordre sur les temps et les
déformations à rupture (At, Ar et déformation circonférentielle en partie courante).},
	number = {16-3664/A},
	institution = {{CEA}/{DEN}/{DANS}/{DMN}/{SRMA}/{LC}2M},
	type = {Note technique},
	author = {Dumerval, Marie},
	date = {2016-10},
	file = {Dumerval - 2016 - Bilan des essais EDGAR multi-crayons réalisés en 2.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/VTPAPMM2/Dumerval - 2016 - Bilan des essais EDGAR multi-crayons réalisés en 2.pdf:application/pdf}
}

@report{coulon_simulation_206,
	location = {{CEA} Saclay},
	title = {Simulation thermique des essais {EDGAR} multicrayons: étude effectuée en 2016 en configuration 1 garde.},
	abstract = {Cette étude concerne la modélisation thermique bidimensionnelle simplifiée au moyen de Cast3M du dispositif {EDGAR} multicrayons qui permet d’étudier le comportement en déformation-rupture de gaines en alliage de zirconium des combustibles nucléaires {REP} en conditions {APRP}.
En configuration 1 garde (cas d’une gaine chauffée non pressurisée située près d’un crayon central chauffé et
pressurisé) , les précisions apportées à la géométrie (position des colonnes et présence du contrepoids) ont peu d’incidence sur le résultat numérique. Par contre, les améliorations apportées à la modélisation de l’échange thermique entre les gaines et la vapeur dans l’espace inter-gaines conduisent à une augmentation significative de l’écart thermique azimutal maximal. Ainsi le ratio expérience-calcul passe de 3. à 1.7.
Pour poursuivre l’analyse, il serait intéressant de :
– compléter l’étude paramétrique (émissivités, conductivité gaine),
– traiter la configuration complète d’{EDGAR} multicrayons comportant 4 gardes chauffées,
– introduire une orientation de l’ensemble des gaines par rapport aux hublots (jusqu’ici les gaines sont alignées avec les hublots latéraux),
– examiner l’influence du caractère absorbant de la vapeur d’eau sur le plan radiatif},
	number = {16-056/A},
	institution = {{CEA}/{DEN}/{DANS}/{DM}2S/{STMF}/{LATF}},
	type = {Note technique},
	author = {Coulon, Nadia},
