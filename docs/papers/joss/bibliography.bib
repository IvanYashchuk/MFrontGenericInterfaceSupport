@report{said_implementation_2016,
	location = {{AREVA} {NP} Lyon},
	title = {Implémentation d’un intégrateur de loi de comportement pour gaines de combustible nucléaire.},
	abstract = {Ce stage a été réalisé à {AREVA} {NP}, dans le groupe Codes et bases de données de l’unité Thermo-mécanique Combustible, pour des applications sur le combustible nucléaire utilisé en {REP} (Réacteurs à Eau Pressurisée) et {REB} (Réacteurs à Eau Bouillante).
Le travail réalisé s’inscrit dans le cadre du développement des codes thermo-mécaniques utilisés pour la conception des crayons de combustible. Il a principalement porté sur l’utilisation d’un intégrateur de loi de comportement, le code mfront. Cet outil permet de générer une librairie effectuant la résolution des équations de la loi avec un schéma numérique et un algorithme de résolutions donnés. Il est également fourni avec un code de résolution numérique sur un point matériel appelé mtest, permettant de tester les librairies ainsi compilées de manière indépendante. Les objectifs du stage ont concerné l’implémentation de lois de comportement de gaines de crayons : d’une part coder une loi matériau spécifique et tester sa validité sous le code à éléments finis {CAST}3M, et d’autre part tester l’implémentation et l’interfaçage des librairies avec le code de simulation thermo-mécanique {GALILEO}.
Après avoir testé la compilation de lois élastique et de Norton ainsi que leur validité grâce à mtest, les principaux résultats suivants ont été obtenus :
• Une loi composée de lois de Norton combinées associées à des calculs de phase a été codée avec mfront puis validée sur des essais expérimentaux avec {CAST}3M.
• Un programme simple de résolution d’équilibre sur un point matériel, utilisant une librairie compilée par mfront, a été développé afin de tester l’appel d’une loi mfront via une interface {FORTRAN}/C++.
• L’implémentation d’une loi de Norton simple dans {GALILEO} a d’abord été effectuée avec succès, avant de tester selon la même méthode d’implémentation une loi matériau {AREVA}. Les résultats de {GALILEO} utilisant les librairies mfront ont alors pu être comparés avec ceux de {GALILEO} non modifié.
Ces résultats et méthodes d’implémentations pourront être utilisés pour élargir le nombre de lois codées avec mfront et ajoutées dans {GALILEO}. L’objectif à terme serait ainsi d’avoir toutes les déformations mécaniques du crayon calculées à l’aide de librairies mfront. Ces travaux pourront être également poursuivis en étudiant l’utilisation de mfront couplée avec le code industriel {ANSYS}.},
	institution = {École Centrale de Lyon},
	type = {Rapport de stage},
	author = {Said, Julien},
	date = {2016},
	file = {Said - 2016 - Implémentation d’un intégrateur de loi de comporte.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/GN4E2GJB/Said - 2016 - Implémentation d’un intégrateur de loi de comporte.pdf:application/pdf}
}

@report{le_saux_implantation_2017,
	location = {Saclay},
	title = {Implantation dans {MFRONT} des modèles {EDGAR} décrivant le comportement thermomécanique des gaines en alliages de Zr en conditions d’{APRP} : application au modèle {EDGAR} M5 v3},
	abstract = {Moyennant un certain nombre d’hypothèses (e.g. viscoplasticité isotrope) et d’approximations (sur la vitesse de déformation notamment), la corrélation 1D {EDGAR} M5 v3 a été transformée en loi de comportement 3D utilisable en grandes transformations (via le formalisme des déformations logarithmiques), puis implantée dans {MFRONT} 3.0.0 (selon une méthode implicite). Les critères de rupture exprimés en déformation viscoplastique limite, densité d’énergie de déformation viscoplastique limite et contrainte limite ont également été transposés en 3D. Les coefficients de la corrélation d’origine ont été conservés. Dans un souci d’efficacité et de robustesse de l’implantation, le traitement de la transformation de phases et de la déformation viscoplastique dans les zones de transition entre les domaines monophasés α Zr et β Zr et le domaine biphasé α Zr + β Zr a été simplifié par rapport ce qui était fait jusqu’à présent pour les simulations « point matériel » effectuées avec {SIDOLO}.
L’implantation du modèle faite dans {MFRONT} a été utilisée pour simuler avec l’outil {PTEST} intégré à {MFRONT} les essais {EDGAR} de la base d’identification et de validation du modèle {EDGAR} M5 v3. La confrontation avec les résultats des simulations effectuées avec {SIDOLO} a montré que la transformation de phases est correctement traitée dans l’implantation {MFRONT}. Comme attendu compte-tenu des hypothèses et des approximations qui ont dû être faites pour transposer la corrélation d’origine en une loi 3D utilisable en grandes transformations, il y a des différences entre les cinétiques de déformation calculées avec {MFRONT}/{PTEST} et celles prévues par les simulations « point matériel » effectuées avec {SIDOLO} à partir de la corrélation 1D d’origine : du fait de l’approximation faite sur la vitesse de déformation notamment, la
cinétique de déformation a tendance à être un peu plus rapide dans le cas des simulations {MFRONT}/{PTEST}, en particulier lorsque les niveaux de déformation atteints sont élevés. Cependant, la comparaison aux données expérimentales montre que les capacités prédictives du modèle ne sont en moyenne pas modifiées de manière très significative. On peut donc raisonnablement considérer que la loi implantée dans {MFRONT}
reprenant les coefficients de la corrélation {EDGAR} M5 v3 d’origine peut être utilisée en l’état, et qu’il n’est à ce stade pas nécessaire de ré-identifier les coefficients du modèle en utilisant l’implantation faite dans {MFRONT}, même si cela serait évidemment plus rigoureux.
Certains essais {EDGAR} ont été simulés avec Cast3M à l’aide d’un modèle 2D en déformation plane généralisée pour illustrer l’utilisation du modèle implanté dans {MFRONT} dans un code de calcul de structure et donner quelques recommandations d’usage dans Cast3M. Les résultats obtenus sont en excellent accord avec ceux des simulations effectuées avec {PTEST}, confirmant notamment que le modèle implanté dans {MFRONT} est correctement traité par Cast3M.
Les fichiers {MFRONT} de l’implantation du modèle {EDGAR} M5 v3 ont été transmis par mail, sous la forme d’un conteneur crypté Zed, aux destinataires de cette note. L’implantation du modèle et les cas-tests associés ont également été reversés dans la base {MFrontMaterials} de la plateforme {PLEIADES}.},
	number = {{NT}/17-3690/A},
	institution = {{DEN}/{DANS}/{DMN}/{SRMA}/{LC}2M},
	type = {Note technique},
	author = {Le Saux, Matthieu},
	date = {2017-03},
	file = {Le Saux - 2017 - Implantation dans MFRONT des modèles EDGAR décriva.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/8539BFIR/Le Saux - 2017 - Implantation dans MFRONT des modèles EDGAR décriva.pdf:application/pdf}
}

