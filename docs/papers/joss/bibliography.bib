	urldate = {2016-08-08},
	keywords = {Cracks, Phase Field, Highly heterogeneous materials, Interfacial damage, Voxel-based models, Microcracking},
	file = {Nguyen et al. - A phase-field method for computational modeling of.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/UVCNNE8Q/Nguyen et al. - A phase-field method for computational modeling of.pdf:application/pdf;ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/I726P6MR/S0045782515003266.html:text/html}
