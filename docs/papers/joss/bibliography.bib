@article{nagtegaal_implementation_1982,
	title = {On the implementation of inelastic constitutive equations with special reference to large deformation problems},
	volume = {33},
	issn = {0045-7825},
	url = {http://www.sciencedirect.com/science/article/pii/0045782582901207},
	doi = {10.1016/0045-7825(82)90120-7},
	abstract = {Methods for finite element analysis of inelastic problems are reviewed. Problems with currently popular methods, based on rate principles of virtual work, are pointed out. A formulation is presented based on the exact (incremental) virtual work equation. The procedure for integration of the constitutive rate equations to obtain incremental constitutive equations is based on the assumption of a constant strain rate (or straight strain path) during the increment. Full Newton-Raphson iteration is used to solve the resulting set of nonlinear equations. The treatment of the constitutive equations in this Newton-Raphson scheme leads to a linearized form which is substantially different from the classical rate equations. The procedure is implemented in a finite element program and its effectiveness is shown in a number of examples.},
	pages = {469--484},
	number = {1},
	journaltitle = {Computer Methods in Applied Mechanics and Engineering},
	shortjournal = {Computer Methods in Applied Mechanics and Engineering},
	author = {Nagtegaal, Joop C.},
	date = {1982-09-01},
	file = {Nagtegaal - 1982 - On the implementation of inelastic constitutive eq.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/9BSI2JHE/Nagtegaal - 1982 - On the implementation of inelastic constitutive eq.pdf:application/pdf;ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/II2ZDR9A/0045782582901207.html:text/html}
}

@article{simo_unified_1985,
