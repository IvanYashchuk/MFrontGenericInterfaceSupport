	urldate = {2018-10-15},
	date = {1996-01-01},
	keywords = {Anisotropy, Finite element method, Endommagement, Modeling, Analyse structurale, Anisotropie, Behavior, Code calcul, Comportement, Computation code, Concrete construction, Construction en béton, Construction structure, Damaging, Endommagement, Mécanique de l' (milieux continus), Essai, Forecast model, Mécanique, Mécanique, génie mécanique, génie civil, Mechanics, Méthode élément fini, Modèle prévision, Modélisation, Structural analysis, Structure construction, Test},
	file = {Fichant - 1996 - Endommagement et anisotropie induite du béton de s.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/J5BIJIMA/Fichant - 1996 - Endommagement et anisotropie induite du béton de s.pdf:application/pdf;Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/UHCE3QXB/1996DENS0015.html:text/html}
