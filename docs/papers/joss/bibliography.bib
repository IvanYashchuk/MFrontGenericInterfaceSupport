	volume = {346},
	issn = {0045-7825},
	url = {http://www.sciencedirect.com/science/article/pii/S0045782518304353},
	doi = {10.1016/j.cma.2018.08.037},
	abstract = {We devise and evaluate numerically a Hybrid High-Order ({HHO}) method for incremental associative plasticity with small deformations. The {HHO} method uses as discrete unknowns piecewise polynomials of order k≥1 on the mesh skeleton, together with cell-based polynomials that can be eliminated locally by static condensation. The {HHO} method supports polyhedral meshes with non-matching interfaces, is free of volumetric locking, and the integration of the behavior law is performed only at cell-based quadrature nodes. Moreover, the principle of virtual work is satisfied locally with equilibrated tractions. Various two- and three-dimensional test cases from the literature are presented including comparison against known solutions and against results obtained with an industrial software using conforming and mixed finite elements.},
	pages = {891--912},
	journaltitle = {Computer Methods in Applied Mechanics and Engineering},
	shortjournal = {Computer Methods in Applied Mechanics and Engineering},
	author = {Abbas, Mickaël and Ern, Alexandre and Pignet, Nicolas},
	urldate = {2019-05-19},
	date = {2019-04-01},
	keywords = {Associative plasticity, Hybrid high-order methods, Polyhedral meshes},
	file = {ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/XX8KNS8M/S0045782518304353.html:text/html;Version soumise:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/9RDKQMC2/Abbas et al. - 2019 - A Hybrid High-Order method for incremental associa.pdf:application/pdf}
}

@article{sicsic_initiation_2014,
	title = {Initiation of a periodic array of cracks in the thermal shock problem: A gradient damage modeling},
	volume = {63},
	issn = {0022-5096},
	url = {http://www.sciencedirect.com/science/article/pii/S0022509613001786},
	doi = {10.1016/j.jmps.2013.09.003},
	shorttitle = {Initiation of a periodic array of cracks in the thermal shock problem},
	abstract = {This paper studies the initiation of cracks in the thermal shock problem through the variational analysis of the quasi-static evolution of a gradient damage model. We consider a two-dimensional semi-infinite slab with an imposed temperature drop on its free surface. The damage model is formulated in the framework of the variational theory of rate-independent processes based on the principles of irreversibility, stability and energy balance. In the case of a sufficiently severe shock, we show that damage immediately occurs and that its evolution follows first a fundamental branch without localization. Then it bifurcates into another branch in which damage localization will take place finally to generate cracks. The determination of the time and mode of that bifurcation allows us to explain the periodic distribution of the so-initiated cracks and to calculate the crack spacing in terms of the material and loading parameters. Numerical investigations complete and quantify the analytical results.},
	pages = {256--284},
	journaltitle = {Journal of the Mechanics and Physics of Solids},
	shortjournal = {Journal of the Mechanics and Physics of Solids},
	author = {Sicsic, Paul and Marigo, Jean-Jacques and Maurini, Corrado},
	urldate = {2019-05-22},
	date = {2014-02-01},
	keywords = {Gradient damage model, Damage mechanics, Variational methods, Energy balance, Rayleigh ratio, Stability and bifurcation, Thermal shock},
	file = {ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/BY5D7V5E/S0022509613001786.html:text/html;Sicsic et al. - 2014 - Initiation of a periodic array of cracks in the th.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/MJZFNJ3F/Sicsic et al. - 2014 - Initiation of a periodic array of cracks in the th.pdf:application/pdf}
}

@thesis{occelli_explicit_2018,
	title = {Explicit dynamics isogeometric analysis : lr b-splines implementation in the radioss solver},
	url = {http://www.theses.fr/2018LYSEI102},
	shorttitle = {Explicit dynamics isogeometric analysis},
	abstract = {L'analyse isométrique s'est révélée être un outil très prometteur pour la conception et l'analyse. Une tâche difficile consiste toujours à faire passer l'{IGA} de concept à un outil de conception pratique pour l'industrie et ce travail contribue à cet effort. Ce travail porte sur l'implémentation de l'{IGA} dans le solveur explicite Altair Radioss afin de répondre aux applications de simulation de crash et d'emboutissage. Pour cela, les ingrédients nécessaires à une intégration native de l'{IGA} dans un code éléments finis traditionnel ont été identifiés et adaptés à l'architecture de code existante. Un élément solide B-Spline et {NURBS} a été développé dans Altair Radioss. Les estimations heuristiques des pas de temps élémentaires ou nodaux sont explorées pour améliorer l'efficacité des simulations et garantir leur stabilité. Une interface de contact existante a été étendue afin de fonctionner de manière transparente avec les éléments finis {NURBS} et de Lagrange. Un raffinement local est souvent nécessaire pour la bonne représentation de champs non linéaires tels que les champs de déformations plastiques. Une analyse est faite en termes de compatibilité pour l'analyse et de mise en oeuvre pour plusieurs bases de fonctions Spline telles que les Hierarchical B-Splines, les Truncated Hierarchical B-Splines, les T-Splines et les Locally Refined B-Splines ({LR} B-Splines). Les {LR} B-Splines sont implémentés. Un schéma de raffinement est proposé et définit un sous-ensemble de raffinements adapté à leur utilisation au sein de Radioss. Le processus de raffinement d’un maillage initialement grossier et régulier est développé au sein du solveur. Il permet à l’utilisateur d’établir du raffinement local par un ensemble d’instructions à fournir dans le jeu de donnée de la simulation. La solution globale est validée sur des cas tests industriels, pour des cas de validation classiquement utilisés pour les codes industriels comme l'emboutissage et les tests de chute.},
	institution = {Lyon},
	type = {thesis},
	author = {Occelli, Matthieu},
	urldate = {2019-05-23},
	date = {2018-11-29},
	keywords = {Finite element, Mécanique, Mécanique, génie mécanique, génie civil, Analyse des éléments finis, Analyse Isogéométrique, Analyse isométrique, B-Spline, Champs de déformations plastiques, Emboutissage, Industrial Solver, Isometric analysis, Local refinement, Plastic deformation fields, Raffinement local, Solveur industriel},
	file = {Occelli - 2018 - Explicit dynamics isogeometric analysis  lr b-spl.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/BM35D7JH/Occelli - 2018 - Explicit dynamics isogeometric analysis  lr b-spl.pdf:application/pdf;Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/RZCGPLIN/2018LYSEI102.html:text/html}
}

@online{derouiche_integration_nodate,
	title = {Intégration simplifiée de modèles de comportement non-linéaire},
	url = {https://www.researchgate.net/publication/327845534_Integration_simplifiee_de_modeles_de_comportement_non-lineaire},
	abstract = {L’objectif principal de cette étude était d’évaluer la pertinence et la robustesse de l’intégration d’un modèle de comportement élasto-viscoplastique présentant une forte non-linéarité dans la filière de dimensionnement à la fatigue thermomécanique.
Actuellement, le groupe {PSA} utilise un {USER} Material ({UMAT}) codé en Fortran pour modéliser le modèle élasto-viscoplastique des aluminiums. Or l’intégration d’un nouveau modèle de comportement via un {UMAT} Fortran passe par une mise en données complexe et génératrice d’erreurs. Des solutions d’intégration numérique rapides ont vu le jour pour faciliter l’intégration des modèles les plus complexes notamment {MFront}.
Comme première étape du travail, nous avons formulé et intégré le modèle de comportement élasto-viscoplastique des aluminiums du groupe {PSA} dans le logiciel {MFront}.
Les cas tridimensionnel et contraintes planes ont été étudiés où nous avons testé les performances de trois méthodes d’intégration différentes. Une fois la formulation et l’intégration réalisées, nous avons validé la justesse de nos équations constitutives avec des calculs sur un point d’intégration. Ensuite, des calculs sur des structures industrielles (éprouvette thermique, culasse monocylindre et culasse trois cylindres) sont effectués sur le code élément fini {ABAQUS} afin de nous assurer de la convergence rapide et systématique du modèle, de vérifier les équations constitutives de ce dernier et de comparer la méthodologie actuelle du groupe {PSA} avec celles proposées par {MFront}. Les différents algorithmes testés sont efficaces et donnent des résultats similaires à celui du {UMAT} Fortran et avec des temps de calcul comparables et des coûts de développement plus réduits.},
	titleaddon = {{ResearchGate}},
	author = {Derouiche, Khouloud},
	urldate = {2019-05-27},
	langid = {english},
	file = {Intégration simplifiée de modèles de comportement .pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/QDIJ7KUU/Intégration simplifiée de modèles de comportement .pdf:application/pdf;Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/8G4AUYZB/327845534_Integration_simplifiee_de_modeles_de_comportement_non-lineaire.html:text/html}
}

