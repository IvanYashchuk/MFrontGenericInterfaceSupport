@report{basini_etudes_2001,
	location = {{CEA} Cadarache},
	title = {Etudes des propriétés mécaniques du combustible {MOX} - Premiers résultats -},
	abstract = {Cette note donne un état d’avancement des travaux conduits dans le cadre de l’action tripartite {EDF}-{FRA}-{CEA} 71CD sur l’étude des propriétés mécaniques du combustible {UPuO} 2 à faibles teneurs en plutonium ({\textless} 15\%).
Cette étude porte essentiellement sur le comportement mécanique à haute température (T = 1500 °C) du combustible {MOX} et, plus particulièrement, sur ses déformations en régime stationnaire en fonction :
- du procédé de fabrication (procédés {COCA}/{MIMAS}),
- de sa teneur en oxyde de plutonium (quantité variant de 0 à 10 \% massique),
- de ses caractéristiques microstructurales (porosités, tailles des grains et répartition du plutonium).
Cette note a pour objectif de présenter les premiers résultats obtenus et d’en dégager des enseignements permettant d’évaluer le comportement mécanique du combustible {MOX} et de le comparer à celui de l’{UO} 2 afin d’orienter la suite des essais mécaniques.
L’ensemble de ces résultats permet de tirer quelques enseignements quant à l’influence du Plutonium, sur le comportement thermomécanique des {MOX}.
On a observé, en effet, une capacité à se déformer des combustibles hétérogènes (Lots {MIMAS}) qui s’accentue avec l’augmentation de la teneur en Plutonium, essentiellement en fluage aux contraintes inférieures à 50 {MPa}.
L’hétérogénéité du {MIMAS} étant renforcée sous irradiation par l’introduction d’une forte porosité dans les amas, la tendance observée laisse à penser que la vitesse de fluage sous flux du combustible {MOX} irradié pourrait être sensiblement supérieure à la vitesse de fluage hors flux du {MOX} vierge, et donc à celle de l’{UO} 2 .
A ce stade de l’étude, le besoin d’initier des actions de compréhension apparaît clairement. Il faudrait, par exemple, étudier l’influence des différentes phases en présence dans le combustible {MIMAS} sur le comportement thermomécanique. L’impact de la variation de certains paramètres comme la taille de grain et la porosité dans ces différentes phases pourrait être regardé. L’objectif étant d’accéder à une compréhension des mécanismes microstructuraux de déformation afin de mieux appréhender le comportement des {MOX} vierges puis celui des {MOX} irradiés.},
	number = {{LMPC} n° 11},
	institution = {{DEN}/{CAD}/{DEC}/{SPUA}/{LMPC}},
	type = {Note technique},
	author = {Basini, Virginie and Richaud,, J.C. and Sladkoff, M.},
	date = {2001-12},
	file = {Basini et al. - 2001 - Etudes des propriétés mécaniques du combustible MO.pdf:/tmp/Basini et al. - 2001 - Etudes des propriétés mécaniques du combustible MO.pdf:application/pdf}
}

@article{ramiere_iterative_2015,
	title = {Iterative residual-based vector methods to accelerate fixed point iterations},
	volume = {70},
	issn = {0898-1221},
	url = {http://www.sciencedirect.com/science/article/pii/S0898122115004046},
	doi = {10.1016/j.camwa.2015.08.025},
	abstract = {Fixed point iterations are still the most common approach to dealing with a variety of numerical problems such as coupled problems (multi-physics, domain decomposition, …) or nonlinear problems (electronic structure, heat transfer, nonlinear mechanics, …). Methods to accelerate fixed point iteration convergence or more generally sequence convergence have been extensively studied since the 1960’s. For scalar sequences, the most popular and efficient acceleration method remains the Δ 2 of Aitken. Various vector acceleration algorithms are available in the literature, which often aim at being multi-dimensional generalizations of the Δ 2 method.

In this paper, we propose and analyse a generic residual-based formulation for accelerating vector sequences. The question of the dynamic use of this residual-based transformation during the fixed point iterations for obtaining a new accelerated fixed point method is then raised. We show that two main classes of such iterative algorithms can be derived and that this approach is generic in that various existing acceleration algorithms for vector sequences are thereby recovered.

In order to illustrate the interest of such algorithms, we apply them in the field of nonlinear mechanics on a simplified “point-wise” solver used to perform mechanical behaviour unit testings. The proposed test cases clearly demonstrate that accelerated fixed point iterations based on the elastic operator (quasi-Newton method) are very useful when the mechanical behaviour does not provide the so-called consistent tangent operator. Moreover, such accelerated algorithms also prove to be competitive with respect to the standard Newton–Raphson algorithm when available.},
	pages = {2210--2226},
	number = {9},
	journaltitle = {Computers \& Mathematics with Applications},
	shortjournal = {Computers \& Mathematics with Applications},
	author = {Ramière, Isabelle and Helfer, Thomas},
	urldate = {2016-09-22},
	date = {2015-11},
	keywords = {Fixed point method, Acceleration iterative technique, Vector sequences, Residual-based method, Nonlinear mechanics, Mechanical behaviours},
	file = {Ramière et Helfer - Iterative residual-based vector methods to acceler.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/73E8ZVDF/Ramière et Helfer - Iterative residual-based vector methods to acceler.pdf:application/pdf;ScienceDirect Full Text PDF:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/97INRFHS/Ramière et Helfer - 2015 - Iterative residual-based vector methods to acceler.pdf:application/pdf;ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/R53T6UFJ/S0898122115004046.html:text/html;ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/BWG9IEZM/S0898122115004046.html:text/html}
}

