	url = {http://www.sciencedirect.com/science/article/pii/S1631072117301900},
	doi = {10.1016/j.crme.2017.09.005},
	abstract = {One of the most successful models for describing the Portevin–Le Chatelier effect in engineering applications is the Kubin–Estrin–{McCormick} model ({KEMC}). In the present work, the influence of dynamic strain ageing on dynamic recovery due to dislocation annihilation is introduced in order to improve the {KEMC} model. This modification accounts for additional strain hardening rate due to limited dislocation annihilation by the diffusion of solute atoms and dislocation pinning at low strain rate and/or high temperature. The parameters associated with this novel formulation are identified based on tensile tests for a C–Mn steel at seven temperatures ranging from 20 °C to 350 °C. The validity of the model and the improvement compared to existing models are tested using 2D and 3D finite element simulations of the Portevin–Le Chatelier effect in tension.},
	pages = {908--921},
	number = {12},
	journaltitle = {Comptes Rendus Mécanique},
	shortjournal = {Comptes Rendus Mécanique},
	author = {Ren, Sicong and Mazière, Matthieu and Forest, Samuel and Morgeneyer, Thilo F. and Rousselier, Gilles},
	date = {2017-12-01},
	keywords = {Plasticity, Identification, Portevin–Le Chatelier effect, Dislocation density, {FEM} simulation, C–Mn steel},
	file = {ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/KMNXSS3U/S1631072117301900.html:text/html}
}

