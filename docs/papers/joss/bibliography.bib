@article{molnar_2d_2017,
	title = {2D and 3D Abaqus implementation of a robust staggered phase-field solution for modeling brittle fracture},
	volume = {130},
	issn = {0168-874X},
	url = {http://www.sciencedirect.com/science/article/pii/S0168874X16304954},
	doi = {10.1016/j.finel.2017.03.002},
	abstract = {In order to model brittle fracture, we have implemented a two and three dimensional phase-field method in the commercial finite element code Abaqus/Standard. The method is based on the rate-independent variational principle of diffuse fracture. The phase-field is a scalar variable between 0 and 1 which connects broken and unbroken regions. If its value reaches one the material is fully broken, thus both its stiffness and stress are reduced to zero. The elastic displacement and the fracture problem are decoupled and solved separately as a staggered solution. The approach does not need predefined cracks and it can simulate curvilinear fracture paths, branching and even crack coalescence. Several examples are provided to explain the advantages and disadvantages of the method. The provided source codes and the tutorials make it easy for practicing engineers and scientists to model diffuse crack propagation in a familiar computational environment.},
	pages = {27--38},
	issue = {Supplement C},
	journaltitle = {Finite Elements in Analysis and Design},
	shortjournal = {Finite Elements in Analysis and Design},
	author = {Molnár, Gergely and Gravouil, Anthony},
	date = {2017-08-01},
	keywords = {Finite element method, Crack propagation, Brittle fracture, Abaqus {UEL}, Phase-field, Staggered solution},
	file = {Molnár et Gravouil - 2017 - 2D and 3D Abaqus implementation of a robust stagge.pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/VM599VQG/Molnár et Gravouil - 2017 - 2D and 3D Abaqus implementation of a robust stagge.pdf:application/pdf;ScienceDirect Snapshot:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/MN6NH4PD/S0168874X16304954.html:text/html}
}

@report{limon_analyse_2017,
	location = {{CEA} Saclay},
	title = {Analyse et réajustement du modèle de déformations en transport et entreposage des gaines M5 pour l’interprétation de l’expérience {FAB}-{CS}},
	abstract = {Le crayon {FABRICE} ‘{FAB}-{CS}’ a subi une expérience dans le dispositif {CROCODILE} pour étudier le comportement d'un crayon étanche à température élevée en cours de transport à sec.
La première interprétation de cet essai a conduit à des écarts entre calculs et mesures de la déformation de la gaine qui pourraient s’expliquer en partie par l’imprécision du modèle de déformation viscoplastique des gaines M5 pour des températures, contraintes et fluences neutroniques voisines de celles de l’essai {FAB}-{CS}. En effet, les versions 2006 et 2009 du modèle ont été identifiées sur un domaine beaucoup plus
large que celui de cet essai, bien que la version 2009 soit a priori mieux adaptée au niveau de fluence du crayon {FAB}-{CS} et aux valeurs atteintes de déformations.
On a évalué les deux versions du modèle sur une partie de leur base d’identification jugée représentative de l’essai {FAB}-{CS} puis on a cherché à améliorer le modèle sur cette base réduite.
Sur cette base, le modèle de 2009, qui a été utilisé pour l’interprétation de l’essai {FAB}-{CS}, est nettement meilleur que celui de 2006 et il estime correctement les effets de la température, de la contrainte et del’écrouissage. On l’améliore un peu sur cette base réduite en ajustant un seul paramètre, pour remonter à 1 le rapport moyen de la déformation calculée à celle mesurée, mais la tendance à sous-estimer la
déformation pour les plus fortes fluences est conservée.
Pour réduire cette tendance, on a testé le modèle réajusté en 2015 sur des essais à haute température (500°C), dont la variable de durcissement d’irradiation est différente de celle des modèles de 2006 et 2009. Sur la base réduite, il est à peine meilleur que le modèle de 2009. En réajustant ce modèle pour remonter à 1 le rapport moyen calcul / mesure, on obtient un modèle quasi aussi valide sur la base réduite que celui obtenu en appliquant un simple facteur au modèle 2009. La diminution du rapport de la
déformation calculée à celle mesurée en fonction du niveau d’irradiation est effectivement atténuée en changeant de variable de durcissement d’irradiation mais l’avantage est très limité sur cette base.
Puisque les deux nouveaux modèles sont presque aussi valides sur la base réduite, on propose de faire l’interprétation de l’essai {FAB}-{CS} avec chacun d’eux. Pour cela, les deux fichiers de paramètres pour {MISTRAL} vont être fournis.},
	number = {{NT}/2017-004/A},
	institution = {{CEA} {DEN}/{DANS}/{DMN}/{SEMI}/{LCMI}},
	type = {Note technique},
	author = {Limon, Roger},
	date = {2017-03},
	file = {Limon - 2017 - Analyse et réajustement du modèle de déformations .pdf:/home/th202608/.zotero/zotero/vzpzva46.default/zotero/storage/BBMMU7GE/Limon - 2017 - Analyse et réajustement du modèle de déformations .pdf:application/pdf}
}

